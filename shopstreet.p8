function _init()
	make_map()
	make_player()
	init_enemies()
end

function _update()
	update_camera()
	update_player()
	update_enemies()
end

function _draw()
	cls()
	draw_map()
	draw_player()
	draw_enemies()
	
	camera(camx,camy)
end

function update_camera()
	if btn(➡️) and 
	   p.x>m.min+48 and 
	   p.x<m.max-72 then
		camx+=p.speed
		m.min+=p.speed
	end
end

-->8
-- player

function make_player()
	p={}
	p.sprite1=7
	p.sprite2=23
	p.inv=false
	p.x=5
	p.y=64
	p.speed=2
end

function update_player()
	if btn(➡️) then
		p.x=mid(m.min,p.x+p.speed,m.max)
		p.inv=false
	end
	if btn(⬅️) then
		p.x=mid(m.min,p.x-p.speed,m.max)
		p.inv=true
	end
	if btnp(⬆️) then
		p.y=mid(40,p.y-8,120)
	end
	if btnp(⬇️) then
		p.y=mid(40,p.y+8,120)
	end
end

function draw_player()
	spr(p.sprite1,p.x,p.y-8,1,1,p.inv,false)
	spr(p.sprite2,p.x,p.y,1,1,p.inv,false)
end

-->8
-- enemy

function init_enemies()
	enemies = {}
	num_shop_lanes = 5
	num_play_lanes =
		16 - num_shop_lanes
	min_spawn_ticks = 0.4
	max_spawn_ticks = 0.8
	next_spawn = 0
	spawn_locs = gen_spawn_locs()
end

function create_enemy(lane)
	local enemy = {}
	enemy.lane = lane
	enemy.x = p.x + 128
	enemy.speed = -0.75
	add(enemies, enemy)
end

function update_enemy(enemy)
	enemy.x += enemy.speed
end

function update_enemies()
	local cur_time = time()
	
	if cur_time >= next_spawn then
		if count(spawn_locs) == 0 then
			spawn_locs = gen_spawn_locs()
		end
		
		local lane_index =
			flr(rnd(count(spawn_locs))) + 1
		lane = spawn_locs[lane_index]
		del(spawn_locs, lane)
		create_enemy(lane)
		
		next_spawn =
			cur_time +
			min_spawn_ticks +
			rnd(max_spawn_ticks)
	end
	
	for i, e in pairs(enemies) do
		update_enemy(e)
	end
end

function draw_enemy(enemy)
	local y_start = (num_shop_lanes + (enemy.lane - 1)) * 8
	
	rectfill(
		enemy.x,
		y_start - 8,
		enemy.x + 7,
		y_start + 7,
		11)
end

function draw_enemies()
	for i, e in pairs(enemies) do
		draw_enemy(e)
	end
end

function gen_spawn_locs()
	local locs = {}
	
	for i = 1, num_play_lanes do
		add(locs, i)
	end
	
	return locs
end
